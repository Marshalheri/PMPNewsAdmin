@model PMPAdmin.ViewModels.ManageNewsCategoryViewModel

@{
    ViewBag.Title = "Home";
}


<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark">WELCOME</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">Index</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->

<ul>

@foreach (var item in Model.AspNetUser)
{
    <li>@item.Email</li>
    <li>@item.FullName</li>
    <li>@item.PasswordHash</li>
}

@foreach (var item in Model.News)
{
    <li>@item.AddedById</li>
    <li>@item.CategoryId</li>
    <li>@item.Details</li>
}

@foreach (var item in Model.NewsCategory)
{
    <li>@item.AddedBy</li>
    <li>@item.Name</li>
    <li>@item.IsActive</li>
}
</ul>
